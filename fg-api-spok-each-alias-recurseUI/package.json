    "@types/ramda": "^0.28.20",
    "chai-sorted": "^0.2.0",
    "cy-spok": "^1.5.2",
    "cypress": "11.2.0",
    "cypress-aliases": "1.4.1",
    "cypress-data-session": "^2.3.1",
    "cypress-each": "^1.13.1",
    "cypress-plugin-api": "^2.6.0",
    "cypress-recurse": "^1.24.0",
    "cypress-should-really": "^1.6.0",
    "find-cypress-specs": "1.19.3",
    "prettier": "2.7.1",
    "ramda": "^0.28.0",
