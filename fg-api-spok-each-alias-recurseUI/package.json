{
  "name": "test-api-example",
  "version": "1.0.0",
  "description": "Practice exercises for testing API calls using Cypress",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "start": "npm start --prefix todomvc",
    "cy:open": "cypress open",
    "dev": "start-test 3000 cypress",
